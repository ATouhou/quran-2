[[{"quran": "In the name of God, the Almighty, the Merciful.", "verse": 0, "surah": 104}], [{"quran": "Woe to every spiteful slanderer", "verse": 1, "surah": 104}, {"quran": "who accumulated wealth and piled it up.", "verse": 2, "surah": 104}, {"quran": "He thinks that his wealth will make him immortal.", "verse": 3, "surah": 104}, {"quran": "Never! He will be thrown into the Destroyer.", "verse": 4, "surah": 104}, {"quran": "And do you know about the Destroyer?", "verse": 5, "surah": 104}, {"quran": "The fire which God kindles,", "verse": 6, "surah": 104}, {"quran": "which reaches across the hearts?", "verse": 7, "surah": 104}, {"quran": "It will enclose them", "verse": 8, "surah": 104}, {"quran": "in outstretched columns.", "verse": 9, "surah": 104}]]